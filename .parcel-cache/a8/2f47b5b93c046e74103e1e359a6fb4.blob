.control__back-btn {
  outline-style: none;
  box-sizing: border-box;
  border: none;
  background-color: transparent;
  cursor: pointer;
  height: 28px;
  width: 28px;
  display: block;
  background-image: url("574dbafa7517896e9c9b2dc444503149");
}

.control__back-btn:active,
.control__back-btn:focus {
  background-image: url("7442821473f3979c9260e4961b15d05b");
}
