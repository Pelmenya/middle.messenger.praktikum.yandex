.chats__create-chat {
  font-family: Inter, sans-serif;
  background-color: transparent;
  border-radius: 8px;
  outline-style: none;
  box-sizing: border-box;
  font-style: normal;
  font-weight: 500;
  font-size: 13px;
  line-height: 13px;
  text-align: center;
  color: #999;
  border: none;
  cursor: pointer;
}

.chats__create-chat::before {
  font-family: "Font Awesome 5 Pro";
  font-weight: 300;
  content: "\f4ad";
  font-size: 18px;
  display: block;
}
