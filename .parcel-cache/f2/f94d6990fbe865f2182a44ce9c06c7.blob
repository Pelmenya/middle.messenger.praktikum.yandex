}

.chats-list {
   width: 100%;
   display: flex;
   flex-direction: column;
   align-items: center;
   overflow-y: auto;
}

.card__number-message {
  border-radius: 50%;
  height: 16px;
  width: 16px;
  color: #fff;
  background-color: #3369f3;
  font-family: Inter;
  font-style: normal;
  font-weight: 500;
  font-size: 11px;
  line-height: 12px;
  display: flex;
  align-items: center;
}

.card__avatar {
  height: 47px;
  width: 47px;
  min-width: 47px;
  max-height: 47px;
  background-color: #efefef;
  background-repeat: no-repeat;
  border-radius: 50%;
  margin-right: 10px;
  background-image: url(ellipse.HASH_REF_5d68aaca3648ef979492a3f89b9d96fb.jpg);
}

.card__text {
  font-family: Inter;
  font-style: normal;
  font-weight: 500;
  font-size: 12px;
  line-height: 15px;
  color: #999;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
