.chats__header {
  width: calc(100% - 20px);
  display: flex;
  align-items: center;
  flex-direction: column;
  align-items: flex-end;
}

.chats__link {
  font-family: Inter;
  font-style: normal;
  font-weight: 500;
  font-size: 12px;
  line-height: 12px;
  color: #999999;
  text-decoration: none;
  display: flex;
  align-items: center;
  height: 18px;
}

.chats__nav {
  margin-top: 20px;
  width: 100%;
  display: flex;
  justify-content: space-between;
}

.chats__nav-btn {
  font-family: Inter, sans-serif;
  background-color: transparent;
  border-radius: 8px;
  outline-style: none;
  box-sizing: border-box;
  font-style: normal;
  font-weight: 500;
  font-size: 12px;
  line-height: 12px;
  text-align: center;
  color: #999;
  border: none;
  cursor: pointer;
  display: flex;
}

.chats__search {
  background: transparent;
  border-radius: 5px;
  border: none;
  font-size: 12px;
  line-height: 12px;
  height: initial;
  padding-top: 10px;
  padding-bottom: 10px;
  padding-left: 20px;
  padding-right: 20px;
  z-index: 1;
}

.chats__search:focus.chats__search-placeholder {
  display: none;
}

.chats__link-text {
  margin-bottom: 2px;
  display: block;
}

.chats__create-chat {
  font-family: Inter, sans-serif;
  background-color: transparent;
  border-radius: 8px;
  outline-style: none;
  box-sizing: border-box;
  font-style: normal;
  font-weight: 500;
  font-size: 13px;
  line-height: 13px;
  text-align: center;
  color: #999;
  border: none;
  cursor: pointer;
}

.chats__create-chat::before {
  font-family: "Font Awesome 5 Pro";
  font-weight: 300;
  content: "\f4ad";
  font-size: 18px;
  display: block;
}


.chats__icon-angle-right::after {
  font-family: "Font Awesome 5 Pro";
  font-weight: 300;
  content: "\f105";
  font-size: 18px;
  display: block;
}

.chats__form {
  width: 100%;
  height: 32px;
  background: #efefef;
  border-radius: 5px;
  display: flex;
  position: relative;
  margin-top: 24px;
  margin-bottom: 12px;
}

.chats__search-placeholder {
  display: block;
  text-align: center;
  font-family: Inter;
  font-style: normal;
  font-weight: 500;
  font-size: 12px;
  line-height: 12px;
  color: #999999;
  position: absolute;
  width: 100%;
  text-align: center;
  top: 10px;
  z-index: 0;
}

.chats__search-placeholder::before {
  font-size: 12px;
  line-height: 12px;
  color: #999999;
  font-family: "Font Awesome 5 Pro";
  content: '\f002';
}

.chats__search:focus + .chats__search-placeholder {
  display: none;
}

.chats {
  flex-direction: column;
  width: 25%;
  border-right: 1px solid #eaeaea; 
}

.chats-list__container {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.chats-list__spliter {
  width: calc(100% - 20px);
  border: none;
  border-top: 1px solid #eaeaea;
  margin: 0;
  margin-top: 2px;
