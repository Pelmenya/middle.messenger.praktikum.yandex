.messages-list__settings {
  outline-style: none;
  box-sizing: border-box;
  font-family: "Font Awesome 5 Pro";
  font-size: 15px;
  line-height: 15px;
  text-align: center;
  border: none;
  background-color: transparent;
  color: #1e1e1e;
  border-radius: 50%;
  height: 25px;
  width: 25px;
  cursor: pointer;
}

.messages-list__settings:active,
.messages-list__settings:focus {
  background-color: #f5f5f5;
  color: #3369f3;
}

