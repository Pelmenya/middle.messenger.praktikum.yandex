body {
  background: #efefef;
}
.body {
  width: 100vw;
  height: 100vh;
  background-color: #fff;
  margin: 0 auto;
  display: flex;
  align-items: center;
  flex-direction: column;
  position: relative;
  display: none;
}
.page-link {
  font-family: Inter;
  font-size: 15px;
  color: maroon;
  position: absolute;
  bottom: 20px;
  right: 20px;
  z-index: 99;
}
.page-link_chat {
  bottom: 200px;
  right: 20px;
}
